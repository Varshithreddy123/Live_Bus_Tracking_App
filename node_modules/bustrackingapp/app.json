{
  "expo": {
    "name": "findabus",
    "slug": "findabus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/logo.png",
    "scheme": "findabus",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.project.livebustracking",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB2FZr208vxtUuEH4NLT87LnnDDDtRsemo"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/TT-Octosquares-Medium.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9e07e561-5875-48af-8480-d8da20a27eb1"
      }
    }
  }
}
